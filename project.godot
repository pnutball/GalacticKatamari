[layer_names]

3d_render/layer_20="Level Geometry"
3d_physics/layer_1="Katamari"
3d_physics/layer_2="Objects"
3d_physics/layer_3="Geom: Floor"
3d_physics/layer_4="Geom: Wall"
3d_physics/layer_16="Level Geometry"

[physics]

3d/run_on_separate_thread=true
common/physics_ticks_per_second=120
common/max_physics_steps_per_frame=16
3d/physics_engine="JoltPhysics3D"
3d/default_gravity=14.0
3d/solver/solver_iterations=32
jolt_3d/sleep/enabled=false
jolt_3d/collisions/areas_detect_static_bodies=true
jolt_3d/continuous_cd/max_penetration=5.0
jolt_3d/limits/max_linear_velocity=1000.0
jolt_3d/limits/max_temporary_memory=64

[rendering]

renderer/rendering_method="mobile"
reflections/sky_reflections/texture_array_reflections=false
reflections/sky_reflections/ggx_samples=8
camera/depth_of_field/depth_of_field_bokeh_shape=0
camera/depth_of_field/depth_of_field_bokeh_quality=0
camera/depth_of_field/depth_of_field_use_jitter=true
