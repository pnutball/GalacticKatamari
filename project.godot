config_version=5

[application]

config/name="Galactic Katamari"
config/name_localized={
"ja": "銀河系塊魂"
}
run/main_scene="res://scenes/debug_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Pnutball/GalacticKatamari"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.png"

[audio]

driver/driver=""

[autoload]

StageLoader="*res://scripts/stage_loader.gd"
FPSCounter="res://scenes/fps_counter.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
